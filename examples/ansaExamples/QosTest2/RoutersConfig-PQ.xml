<?xml version="1.0"?>
<Routers>

  <Router id="192.168.3.1"> <!-- R1 -->
    <Interfaces>
     <Interface name="ppp0">
        <IPAddress>192.168.3.1</IPAddress>
        <Mask>255.255.255.0</Mask>
        <OutputQueue type="PQ">
          <PriorityQueueList>10</PriorityQueueList>
        </OutputQueue>
      </Interface>
      <Interface name="eth0">
        <IPAddress>172.16.100.1</IPAddress>
        <Mask>255.255.255.0</Mask>
      </Interface>
    </Interfaces>
    <PriorityQueueLists>
      <PriorityQueueList id="10">
        <DefaultQueue>Low</DefaultQueue>
        <HighQueue>
          <Classifier type="DSCP">
            <value>ef</value>
          </Classifier>
        </HighQueue>
        <MediumQueue>
          <Lenght>20</Lenght>
          <Classifier type="ACL">
            <acl>110</acl>
          </Classifier>
        </MediumQueue>
        <LowQueue>
          <Lenght>100</Lenght>
        </LowQueue>
      </PriorityQueueList>
    </PriorityQueueLists>
    <Routing>  
      <Static>
        <Route>
          <NetworkAddress>172.16.200.0</NetworkAddress>
          <NetworkMask>255.255.255.0</NetworkMask>
          <ExitInterface>ppp0</ExitInterface>
        </Route>
      </Static>
    </Routing>
    <ACLs>
      <ACL no="110">
        <entry seq_no="10">
          <action>permit</action>
          <IP_src>0.0.0.0</IP_src>
          <IP_dst>0.0.0.0</IP_dst>
          <WC_src>255.255.255.255</WC_src>
          <WC_dst>255.255.255.255</WC_dst>
          <port_op_src></port_op_src>
          <port_op_dst></port_op_dst>
          <port_beg_src></port_beg_src>
          <port_end_src></port_end_src>
          <port_beg_dst></port_beg_dst>
          <port_end_dst></port_end_dst>
          <protocol>tcp</protocol>
          <orig>access-list 110 permit tcp any any</orig>
        </entry>
      </ACL>
    </ACLs>
  </Router>
  
  <Router id="192.168.3.2"> <!-- R2 -->
    <Interfaces>
     <Interface name="ppp0">
        <IPAddress>192.168.3.2</IPAddress>
        <Mask>255.255.255.0</Mask>
        <OutputQueue type="WFQ">
          <HoldQueueLength>1000</HoldQueueLength>
          <CDT>64</CDT>
          <DynamicQueues>256</DynamicQueues>
        </OutputQueue>
      </Interface>
      <Interface name="eth0">
        <IPAddress>172.16.200.1</IPAddress>
        <Mask>255.255.255.0</Mask>
      </Interface>
    </Interfaces>
    <Routing>
      
      <Static>
        <Route>
          <NetworkAddress>172.16.100.0</NetworkAddress>
          <NetworkMask>255.255.255.0</NetworkMask>
          <NextHopAddress>192.168.3.1</NextHopAddress>
        </Route>
      </Static>
    </Routing>
  </Router>

</Routers>
    <PriorityQueueLists>
      <PriorityQueueList id="10">
        <HighQueue>
          <Classifier type="DSCP">
            <value>ef</value>
          </Classifier>
        </HighQueue>
        <MediumQueue>
          <Classifier type="ACL">
            <acl>110</acl>
          </Classifier>
        </MediumQueue>
        <NormalQueue>
          <Lenght>100</Lenght>
        </NormalQueue>
      </PriorityQueueList>
    </PriorityQueueLists>
    <ACLs>
      <ACL no="110">
        <entry seq_no="10">
          <action>permit</action>
          <IP_src>0.0.0.0</IP_src>
          <IP_dst>0.0.0.0</IP_dst>
          <WC_src>255.255.255.255</WC_src>
          <WC_dst>255.255.255.255</WC_dst>
          <port_op_src></port_op_src>
          <port_op_dst></port_op_dst>
          <port_beg_src></port_beg_src>
          <port_end_src></port_end_src>
          <port_beg_dst></port_beg_dst>
          <port_end_dst></port_end_dst>
          <protocol>tcp</protocol>
          <orig>access-list 110 permit tcp any any</orig>
        </entry>
      </ACL>
    </ACLs>    
    <Routing>
      <Ospf>
        <RFC1583Compatible />
        <Areas>
          <Area id="0.0.0.0">
            <Networks>
              <Network>
                <IPAddress>192.168.0.0</IPAddress>
                <Wildcard>0.0.255.255</Wildcard>
              </Network>
            </Networks>
          </Area>
        </Areas>
      </Ospf>
    </Routing>
  </Router>
  

        <OspfAuthenticationType>Null</OspfAuthenticationType>
        <OspfAuthenticationKey>0x00</OspfAuthenticationKey>
        <OutputQueue type="PQ">
          <PriorityQueueList>10</PriorityQueueList>
        </OutputQueue>
      </Interface>
      <Interface name="ppp1">
        <IPAddress>192.168.0.17</IPAddress>
        <Mask>255.255.255.252</Mask>

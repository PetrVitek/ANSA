ifconfig:

# ppp card 0 of router - connected to DUT
name: ppp0
  inet_addr: 192.168.1.2
  Mask: 255.255.255.252
  Groups: 224.0.0.5
  MTU: 1500
  Metric: 1
  POINTTOPOINT MULTICAST

  
# ethernet card 0 of router - connected to Collector
name: eth0
  inet_addr: 192.168.1.9
  Mask: 255.255.255.252
  Groups: 224.0.0.5
  MTU: 1500
  Metric: 1
  POINTTOPOINT MULTICAST

# ethernet card 1 of router - connected to N2
name: eth1
  inet_addr: 192.168.2.1
  Mask: 255.255.255.0
  Groups: 224.0.0.5:224.0.0.6
  MTU: 1500
  Metric: 1
  BROADCAST MULTICAST


ifconfigend.

route:

224.0.0.0 * 240.0.0.0   H 0 ppp0
224.0.0.0 * 240.0.0.0   H 0 eth0
224.0.0.0 * 240.0.0.0   H 0 eth1

routeend.
